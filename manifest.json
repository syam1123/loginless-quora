{

  "manifest_version": 2,
  "name": "Loginless Quora",
  "version": "1.0",

  "description": "View answers on Quora without login",

  "icons": {
    "48": "icons/explore-48.png"
  },

  "browser_action": {
    "default_icon": "icons/electric-7.png",
    "default_title": "Quora without login",
    "default_popup": "popup/reveal.html"
  },

  "content_scripts": [
    {
      "matches": ["*://www.quora.com/*"],
      "js": ["script.js"]
    }
  ],

    "web_accessible_resources": [
    "icons/twitter-15.png",
    "icons/linkedin-15.png",
    "icons/facebook-15.png"
  ]

}